{
  "name": "bpmn-moddle",
  "version": "5.1.5",
  "description": "A moddle wrapper for BPMN 2.0",
  "scripts": {
    "build": "babel -d . src",
    "all": "run-s lint lib.commonjs/test",
    "lint": "eslint src",
    "dev": "npm lib.commonjs/test -- --watch",
    "test": "mocha --reporter=spec --recursive lib.commonjs/test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bpmn-io/bpmn-moddle"
  },
  "keywords": [
    "bpmn",
    "moddle",
    "bpmn20",
    "meta-model"
  ],
  "author": {
    "name": "Nico Rehwaldt",
    "url": "https://github.com/Nikku"
  },
  "contributors": [
    {
      "name": "bpmn.io contributors",
      "url": "https://github.com/bpmn-io"
    }
  ],
  "license": "MIT",
  "sideEffects": false,
  "devDependencies": {
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "chai": "^4.1.2",
    "cmof-parser": "^0.2.0",
    "eslint": "^4.11.0",
    "eslint-plugin-bpmn-io": "^0.4.1",
    "mocha": "^4.0.1",
    "npm-run-all": "^4.1.2",
    "xsd-schema-validator": "^0.5.0"
  },
  "dependencies": {
    "min-dash": "^3.0.0",
    "moddle": "github:tylerjwatson/moddle",
    "moddle-xml": "github:tylerjwatson/moddle-xml"
  }
}
